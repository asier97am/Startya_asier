<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.asier.aranda.startya">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/sya_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/sya_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/SYAThemeAppBar">
        <activity
            android:name=".SYABienvenida"
            android:exported="true"
            android:theme="@style/SYAThemeSplash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SYAIngreso"
            android:exported="true"
            android:parentActivityName=".SYABienvenida">
        </activity>
        <activity
            android:name=".SYARegistro"
            android:exported="true"
            android:parentActivityName=".SYAIngreso">
        </activity>
    </application>

</manifest>